<!--                                        android:text="Motion"-->
<!--                                        android:textColor="@color/white"-->
<!--                                        android:textSize="16sp" />-->
<!--                                </com.facebook.shimmer.ShimmerFrameLayout>-->
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/photo_drip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="17dp"
                                android:layout_toRightOf="@+id/photo_motion">

                                <ImageView
                                    android:id="@+id/img_drip"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/photo_drip" />

<!--                                <com.facebook.shimmer.ShimmerFrameLayout-->
<!--                                    android:id="@+id/b"-->
<!--                                    android:layout_width="wrap_content"-->
<!--                                    android:layout_height="wrap_content"-->
<!--                                    android:layout_alignBottom="@+id/img_drip"-->
<!--                                    android:layout_marginLeft="20dp"-->
<!--                                    android:layout_marginBottom="10dp"-->
<!--                                    app:shimmer_base_alpha="1"-->
<!--                                    app:shimmer_base_color="@color/black"-->
<!--                                    app:shimmer_colored="true"-->
<!--                                    app:shimmer_duration="2500"-->
<!--                                    app:shimmer_highlight_color="#919191">-->

<!--                                    <TextView-->
<!--                                        android:layout_width="wrap_content"-->
<!--                                        android:layout_height="wrap_content"-->
<!--                                        android:fontFamily="@font/volte_medium"-->
<!--                                        android:includeFontPadding="false"-->
<!--                                        android:text="Drip"-->
<!--                                        android:textColor="@color/black"-->
<!--                                        android:textSize="16sp" />-->
<!--                                </com.facebook.shimmer.ShimmerFrameLayout>-->
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/photo_pixlab"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="17dp"
                                android:layout_marginRight="17dp"
                                android:layout_toRightOf="@+id/photo_drip">

                                <ImageView
                                    android:id="@+id/img_pixlab"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/photo_pixlab" />

<!--                                <com.facebook.shimmer.ShimmerFrameLayout-->
<!--                                    android:id="@+id/c"-->
<!--                                    android:layout_width="wrap_content"-->
<!--                                    android:layout_height="wrap_content"-->
<!--                                    android:layout_alignBottom="@+id/img_pixlab"-->
<!--                                    android:layout_marginLeft="20dp"-->
